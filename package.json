{
  "name": "stack-composer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .js,.ts,.tsx && markdownlint docs/**/*.md && vale docs/",
    "mdlint": "markdownlint-cli2 \"**/*.md\" \"#**/node_modules/**\" \"#**/dist/**\" \"#**/target/**\" \"#**/book/**\" \"#**/.git/**\" \"#**/.husky/**\" \"#**/.vscode/**\" \"#**/.idea/**\" \"#.trunk/**\" \"#**/.trunk/**\"",
    "mdlint:fix": "markdownlint-cli2 \"**/*.md\" \"#**/node_modules/**\" \"#**/dist/**\" \"#**/target/**\" \"#**/book/**\" \"#**/.git/**\" \"#**/husky/**\" \"#**/.vscode/**\" \"#**/.idea/**\" \"#.trunk/**\" \"#**/.trunk/**\" --fix",
    "test": "vitest run",
    "format": "prettier --write .",
    "preview-wizard": "vite --config vite.wizard.config.ts",
    "rust:build": "cargo build --manifest-path rust-backend/Cargo.toml",
    "rust:test": "cargo test --manifest-path rust-backend/Cargo.toml",
    "rust:run": "cargo run --manifest-path rust-backend/Cargo.toml",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@stryker-mutator/core": "9.0.1",
    "@testing-library/jest-dom": "6.6.3",
    "@types/node": "24.0.3",
    "@vitejs/plugin-react": "4.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "dagre": "0.8.5",
    "eslint": "9.29.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "markdown-link-check": "3.13.7",
    "markdownlint-cli": "0.45.0",
    "prettier": "3.5.3",
    "react-arborist": "3.4.3",
    "reactflow": "11.11.4",
    "tippy.js": "6.3.7",
    "trunk": "1.1.0",
    "vite": "6.3.5",
    "vitest": "3.2.4",
    "zod": "3.25.67"
  },
  "dependencies": {
    "@supabase/functions-js": "2.4.4",
    "axios": "1.10.0",
    "inquirer": "12.6.3",
    "node-fetch": "3.3.2",
    "react": "19.1.0",
    "react-d3-tree": "3.6.6",
    "react-dom": "19.1.0"
  },
  "husky": {
    "deprecated": false
  }
}
